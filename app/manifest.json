{
  "manifest_version": 2,
  "name": "Basic Chrome Native Messaging Terminal",
  "version": "1.0",
  "browser_action":{
    "default_popup":"popup.html",
    "default_icon":"icon.png"
  },
  "background" : {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "nativeMessaging",
    "webNavigation"
  ]
}
